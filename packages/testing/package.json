{
  "name": "@hexmcp/testing",
  "version": "0.0.0",
  "description": "Test fixture coverage system for MCP Server Framework with comprehensive protocol validation",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./fixtures": {
      "types": "./dist/fixtures/index.d.ts",
      "import": "./dist/fixtures/index.js"
    },
    "./harness": {
      "types": "./dist/harness/index.d.ts",
      "import": "./dist/harness/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "fixtures",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test-fixtures": "node dist/cli/test-fixtures.js",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "testing",
    "fixtures",
    "test-harness",
    "protocol-validation",
    "json-rpc",
    "server",
    "framework",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hexmcp/codec-jsonrpc": "workspace:*",
    "@hexmcp/core": "workspace:*",
    "@hexmcp/transport": "workspace:*",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.7",
    "jest": "^30.0.3",
    "ts-jest": "^29.4.0",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}
